using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace W5P
{
    internal class Program
    {
        static void Main(string[] args)
        {
            int count = 0,
                result = 0,
                firstNum = 37;
             count++;
             result = count++ * --firstNum + 100;
             System.Console.Write(result);
             Console.Read();
        }
    }
}
