using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace W9
{
    internal class Program
    {
        static void Main(string[] args)
        {
            int counter = 0;
             do
             {
                 Console.WriteLine(counter + "\t" + Math.Pow(counter, 2));
                 counter--;
             } while (counter > 0);
             Console.Read();
            
        }
    }
}
